<div class="home-page">
    <div class="header">
        <a class="home link" href="<%= new_session_url %>">Inventory Tracker</a>
        <form class="horizontal-form" action="<%= session_url %>" method="post">
            <%= auth_token %>

            <label for="email">Email</label>
            <input type="text" id="email" name="user[email]">

            <label for="password">Password</label>
            <input type="password" id="password" name="user[password]">

            <input class="submit" type="submit" value="Login">
        </form>
    </div>
    <div class="home-body">
        <a class="sign-up" href="<%= new_user_url %>">Sign up!</a>
    </div>
</div>